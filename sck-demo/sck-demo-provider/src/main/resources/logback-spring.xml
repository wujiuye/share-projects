<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd}-%d{HH:mm:ss} [%thread] [%X{requestId}] %-5level %logger{35} - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="CONSOLE" />
    </root>
    <logger name="com.wujiuye.sck" additivity="false" level="info">
        <appender-ref ref="CONSOLE" />
    </logger>
</configuration>
